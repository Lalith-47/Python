   "source": [
    "def fact(a):\n",
    "    if  a<=1:\n",
    "        return a\n",
    "    return a*fact(a-1)\n",
    "\n",
    "a=int(input(\"Enter the value: \"))\n",
    "print('Factorial of',a,'is' ,fact(a))\n"
